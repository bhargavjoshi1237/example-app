{
  "version": 2,
  "builds": [
    {
      "src": "/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "artisan",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/build/assets/(.*)",
      "dest": "/public/build/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "outputDirectory": "public"
}
